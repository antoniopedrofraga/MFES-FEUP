class ConfigGenerator
types
			public string = Utilities`string;
			public config = Utilities`config;
operations
		public generateValidConfigs: Model ==> seq of config 
						generateValidConfigs(model) == 
						(
								dcl root : Feature := model.getRoot();
								dcl searcher : ConfigSearcher := new ConfigSearcher(root);
								return searcher.getValidConfigs();
						);
end ConfigGenerator